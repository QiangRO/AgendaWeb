@page
@model Laboratorio_web_15_8_2022.Pages.Categorias.EditarModel
@{
    ViewData["Title"] = "Editar Categoria";
}
<div class="card">
    <div class="card-header">
        <h2 class="text-info">Editar categoria</h2>
    </div>
    <div class="card-body">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Categoria.Id"/>
            <div class="container" style="padding: 30px">
                <div class="form-group">
                    <div class="row">
                        <div class="col-sm-2"></div>
                    </div>
                </div>
                <div class="form-group">
                </div>
            </div>
        </form>
    </div>
</div>